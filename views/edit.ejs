  <body>
    <h1>EDIT DATA</h1>
    <div id=menu>
  <a href="/">Home</a>
<br>
<br>


  <form action="/edit/<%= data.id %>" method="post">
   ID: <br> <input type="text" name = "id" value="<%= data.id %>" disabled> <br>
   String: <br> <input type="text" name = "string" value="<%= data.string %>"> <br>
   Integer: <br> <input type="text" step="1" name = "integer" value="<%= data.integer %>"> <br>
   Float: <br> <input type="text"  name = "float" value="<%= data.float %>"> <br>
   Date: <br> <input type="date" name = "date"  value="<%= data.date %>"> <br>
   Boolean:
   <select name="boolean">
     <option value="true"<%= data.boolean == "true" ? 'selected':'' %>>True</option>
   <option value="false"<%= data.boolean == "false" ? 'selected':'' %>>False</option>

 </select>
 <br>
<input type="submit" value = "Update"> <br>
