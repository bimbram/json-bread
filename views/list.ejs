<!DOCTYPE html>
<html>

  <head>

    <meta charset="utf-8">
    <title></title>
  </head>
  <body>


<br>


  <table>
    <thead>
      <tr>
      <th>ID</th>
      <th>String</th>
      <th>Integer</th>
      <th>Float</th>
      <th>Date</th>
      <th>Boolean</th>
      <th>Actions</th>
    </tr>
    </thead>
    <tbody>
      <% if(data.length >=0) { %>
       <% for(var i = 0; i<data.length; i++) {%>
         <tr>
           <td> <%= data[i].id %> </td>
           <td> <%= data[i].string %> </td>
           <td> <%= data[i].integer %> </td>
           <td> <%= data[i].float %> </td>
           <td> <%= data[i].date %> </td>
           <td> <%= data[i].bool %> </td>
           <td><button> <a href='/edit/<%= data[i].id %>'>Edit</a></button> <button onclick="return confirm('Are you sure you want to delete this item?')";><a href='/delete/<%= data[i].id %>'>Delete</a></button></td>
        </tr>
        <% } %>
      <% } else { %>
        <p>no data</p>
      <% }%>
    </tbody>

  </table>
  <a href='/add'>Tambah</a>
  </form>

  </body>
</html>
